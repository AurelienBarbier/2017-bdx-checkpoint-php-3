{% extends 'base.html.twig' %}


{% block body %}
    <div class="row">
        <div class="col s6 offset-s3">
            <ul class="collection with-header">
                <li class="collection-header"><h4>Notes</h4></li>
                {% for tvshow in tvShows %}
                <li class="collection-item"><div class="center-align"><div class="left"><span>{{ tvshow.moy }} <i class="small material-icons">grade</i></span></div><span class="">{{ tvshow.obj.name }}</span><a href="{{ path('wcs_tv_show_manager_episode', {id : tvshow.obj.id}) }}" class="secondary-content"><i class="material-icons">send</i></a><br></div></li>
                {% endfor %}
            </ul>
        </div>
    </div>
{% endblock %}